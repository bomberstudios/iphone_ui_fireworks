function setDefaultValues() {  var values = new Array();  // Label  values.push({ name:"Label", type:"text", value:"x" });  // Color  values.push({ name:"Color", type:"ComboBox", value:"Light,Light,Dark"});  Widget.elem.customData["currentValues"] = values;}function applyCurrentValues() {  var values = Widget.elem.customData["currentValues"];  var state  = values[1].value.split(",")[0].toString();  Widget.GetObjectByName("txt_light").visible = (state == "Light");  Widget.GetObjectByName("light").visible = (state == "Light");  Widget.GetObjectByName("txt_dark").visible = (state == "Dark");  Widget.GetObjectByName("dark").visible = (state == "Dark");  Widget.GetObjectByName("txt_light").textChars = values[0].value;  Widget.GetObjectByName("txt_dark").textChars = values[0].value;}switch (Widget.opCode) {  case 1:    setDefaultValues();    break;  case 2:    applyCurrentValues();    break;}