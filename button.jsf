function setDefaultValues(){	var values = new Array();	// values[0]	values.push({ name:"Label", type:"text", value:"Label" });	// values[1]	values.push({ name:"Wadus", type:"Boolean", value:"true"});	// values[2]	values.push({ name:"Type", type:"ComboBox", value:"Grey,Grey,Blue,Light Blue,Red"});	Widget.elem.customData["currentValues"] = values;}function applyCurrentValues(){	var values = Widget.elem.customData["currentValues"];	var state  = values[2].value.split(",")[0].toString();	if (values[1].value.toString() == "true") {		Widget.GetObjectByName("active").visible 		= true;		Widget.GetObjectByName("inactive").visible 		= false;		Widget.GetObjectByName("active").textChars 		= values[0].value;	} else {		Widget.GetObjectByName("active").visible 		= false;		Widget.GetObjectByName("inactive").visible 		= true;		Widget.GetObjectByName("inactive").textChars 	= values[0].value;	}		Widget.GetObjectByName("dark_grey").visible 	= (state == "Grey");		Widget.GetObjectByName("light_blue").visible 	= (state == "Light Blue");		Widget.GetObjectByName("blue").visible 			= (state == "Blue");		Widget.GetObjectByName("red").visible 			= (state == "Red");}switch (Widget.opCode){	case 1: setDefaultValues();   break;	case 2: applyCurrentValues(); break;}