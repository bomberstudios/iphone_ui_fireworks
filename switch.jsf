function setDefaultValues(){	var values = new Array();	// values[1]	values.push({ name:"Type", type:"ComboBox", value:"ON,ON,OFF"});	Widget.elem.customData["currentValues"] = values;}function applyCurrentValues(){	var values = Widget.elem.customData["currentValues"];	var state  = values[0].value.split(",")[0].toString();	Widget.GetObjectByName("button_ON").visible 	= (state == "ON");	Widget.GetObjectByName("button_OFF").visible 	= (state == "OFF");}switch (Widget.opCode){	case 1: setDefaultValues();   break;	case 2: applyCurrentValues(); break;}