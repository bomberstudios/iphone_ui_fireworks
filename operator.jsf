function setDefaultValues() {  var values = new Array();  values.push({ name:"Operator Name", type:"text", value:"Operator" });  Widget.elem.customData["currentValues"] = values;}function applyCurrentValues() {  var values = Widget.elem.customData["currentValues"];  Widget.GetObjectByName("txt").textChars = values[0].value;}switch (Widget.opCode) {  case 1:    setDefaultValues();    break;  case 2:    applyCurrentValues();    break;}