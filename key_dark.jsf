function setDefaultValues() {  var values = new Array();  // Label  values.push({ name:"Label", type:"text", value:"x" });  // Color  values.push({ name:"Color", type:"ComboBox", value:"Grey,Grey,Blue"});  Widget.elem.customData["currentValues"] = values;}function applyCurrentValues() {  var values = Widget.elem.customData["currentValues"];  var state  = values[1].value.split(",")[0].toString();  Widget.GetObjectByName("grey").visible = (state == "Grey");  Widget.GetObjectByName("blue").visible = (state == "Blue");  Widget.GetObjectByName("txt").textChars = values[0].value;}switch (Widget.opCode) {  case 1:    setDefaultValues();    break;  case 2:    applyCurrentValues();    break;}